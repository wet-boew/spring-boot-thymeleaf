@Library(["ised-cicd-lib", "ised-cicd-lib-api"]) _

pipeline {
  agent {
    label 'maven'
  }

  tools {
    jdk 'openjdk-11'
  }
   	
    options {
        disableConcurrentBuilds()
    }
    
    stages {
		stage('release') {
			steps {
				script{
					release.releaseMavenLibrary()
				}
			}
		}
    }
}
